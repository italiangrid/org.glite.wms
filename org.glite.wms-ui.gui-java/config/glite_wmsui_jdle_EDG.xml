   </edg:attribute>
   <edg:attribute>
	<edg:name>RunTimeEnvironment</edg:name><edg:type><edg:STRING/></edg:type><edg:multi/>
   </edg:attribute>
   <edg:attribute>
	<edg:name>TotalCPUs</edg:name><edg:type><edg:INTEGER/></edg:type>
   </edg:attribute>

   <!--"TotalJobs", "WrostTraversalTime", "GlobusResourceContactString", "AuthorizedUser"
        INTEGER, ABSTIME, STRING, STRING-->
   <edg:attribute>
	<edg:name>TotalJobs</edg:name><edg:type><edg:INTEGER/></edg:type>
   </edg:attribute>
   <edg:attribute>
	<edg:name>WorstTraversalTime</edg:name><edg:type><edg:SECONDS/></edg:type>
