<project name="WMS UI API python component common properties">

	<!-- Include build properties to allow overwriting
	     of properties for subsystem                    -->
	<property file="project/build.properties" />	

	<!-- ======================================================
          Define corresponding subsystem properties
		 ====================================================== -->

	<!-- Subsystem name -->
	<property name="subsystem.name" value="${wms-ui.subsystem.name}"/>
		
	<!-- Subsystem prefix -->
	<property name="subsystem.prefix" value="${wms-ui.subsystem.prefix}"/>

	<!-- ======================================================
          Define component properties
		 ====================================================== -->
		 		
	<!-- Component name prefix -->
	<property name="component.prefix" value="api-python" />
			
	<!-- ======================================================
          Define general component properties
		 ====================================================== -->
	
	<import file="${component.general.properties.file}" />
						
	<!-- ======================================================
		 Define extra properties here ...
		 ====================================================== -->

	<property name="m4files.list" value="globus.m4 classads.m4 boost.m4 glite.m4 cppunit.m4 expat.m4 ares.m4 swig.m4 python.m4 glite_wmsutils.m4 glite_jdl.m4 glite_wms.m4 glite_lb.m4 glite_dgas.m4 glite_dgas_common.m4 glite_security.m4  glite_service_discovery.m4"/>


</project>
