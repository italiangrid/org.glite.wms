<project name="Test3 Wms Subsystem common properties">

	<!-- ======================================================
		 Main properties file for all EGEE Test3 WMS modules
		 ====================================================== -->
	
	<!-- Subsystem name -->
	<property name="subsystem.name" value="${test3.wms.subsystem}"/>
	
	<!-- Package prefix -->
	<property name="package.prefix" value="${application.prefix}-test3-wms"/>
	
	<!-- Subsystem build directory -->
	<property name="subsystem.project.dir" location="${module.dir}/../${subsystem.name}/project" />
	
	<!-- Subsystem dependencies file -->
	<property name="subsystem.dependencies.file" value="${subsystem.project.dir}/dependencies.properties" />

</project>